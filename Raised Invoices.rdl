    <DataSet Name="DateParameter">
      <Query>
        <DataSourceName>Embedded</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DateOptions">
            <Value>=Parameters!DateOptions.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>If @DateOptions = 1
Begin
Select Null as Date
End
If @DateOptions = 0
Begin
SELECT DATEADD(day, DATEDIFF(day, 0, GETDATE()), 0)
End</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
