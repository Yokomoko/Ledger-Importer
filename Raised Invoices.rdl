  <DataSets>
    <DataSet Name="SalesOrdersByDate">
      <Query>
        <DataSourceName>Embedded</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ValueOption">
            <Value>=Parameters!ValueOption.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateOptions">
            <Value>=Parameters!DateOptions.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateFrom">
            <Value>=Parameters!DateFrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateTo">
            <Value>=Parameters!DateTo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
	UniqueID,
	Date,
	CustRef,
	CustName,
	SiteName,
	DueDate,
	GL,
	GLDescription,
	JonasGroup,
	Case when GL = 99999 then 'Great Plains Total' else JonasGroupName end as JonasGroupName,
	MaintenanceType,
	MaintTypeDescription,
	ReportingDescription,
	ItemDescription,
	Case @ValueOption
		when 0 then SUM(QtyValue)
		when 1 then SUM(NetValue)
		when 2 then SUM(GrossValue)
		when 3 then SUM(ProfitValue)
	End as Value,

	Case @ValueOption
		when 0 then 'Qty'
		when 1 then 'Net'
		when 2 then 'Gross'
		when 3 then 'Profit'
	End as ValueName,
	InvoiceNo,
CustOrderNo,
UniqueId2
from 
	saleledgerextended
where 
	ImportType = 'Great Plains' and
	(@DateOptions = 0 and Date between @DateFrom and @DateTo or @DateOptions = 1) and
	EntryType in ('Invoice', 'Return')
	and (CustRef is not null or CustRef != '')
group by
	UniqueID,
	ItemDescription,
	Date,
	CustRef,
	DueDate,
	GL,
	GLDescription,
	ItemDescription,
	InvoiceNo,
	JonasGroup,
	JonasGroupName,
	MaintenanceType,
	MaintTypeDescription,
	ReportingDescription,
	CustOrderNo,
	CustName,
	SiteName,
	UniqueId2
order by 
	DatePart(month,DueDate) + Datepart(yyyy,DueDate), 
	InvoiceNo Desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
