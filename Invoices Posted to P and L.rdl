  <DataSets>
    <DataSet Name="SalesOrdersByDate">
      <Query>
        <DataSourceName>Embedded</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DateFrom">
            <Value>=Parameters!DateFrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateTo">
            <Value>=Parameters!DateTo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
	JournalEntry,
	TrxDate as [Date],
	OriginatingTrxDate as [OriginatingDate], 
	JonasGroup as JonasGroupNo,
	GroupName as JonasGroupName,
	ReportingDescription,
	case when DebitAmount = 0 then CreditAmount when CreditAmount = 0 then DebitAmount *-1  end as Value,
	Case when OriginatingDocumentNo = '' then Reference else OriginatingDocumentNo end as Reference, 
	Case when OriginatingMasterID = '' then '-1' else OriginatingMasterID end as CustRef,
	Case when OriginatingMasterName = '' then 'Deferred Revenue' else OriginatingMasterName end as CustName,
	Voided,
	Case when OriginatingMasterID = '' then 0 else 1 end as GroupingReference
from 
	PostedInvoicesExtended
Where 
	[TrxDate] between @DateFrom and @DateTo</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="JournalEntry">
          <DataField>JournalEntry</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="Date">
          <DataField>Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="CustRef">
          <DataField>CustRef</DataField>
