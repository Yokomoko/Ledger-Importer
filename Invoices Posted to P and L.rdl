  <Code>Function GetPeriodFrom(returnValue As String)

        'THISWEEK
        'LASTWEEK
        'LASTMONTH
        'THISMONTH
        'LASTYEAR
        'THISYEAR
        'THISWEEKLASTYEAR

        Dim d As DateTime = Today()

        'Internal, manipulated returnValue
        Dim rVI As String = returnValue.ToUpper

        Select Case rVI
            Case "THISWEEK"
                Dim firstDayOfWeek As DateTime
                firstDayOfWeek = d.AddDays(DayOfWeek.Monday - d.DayOfWeek)
                Return firstDayOfWeek
            Case "LASTWEEK"
                Dim firstDayOfWeek As DateTime
                firstDayOfWeek = d.AddDays(DayOfWeek.Monday - d.DayOfWeek).AddDays(-7)
                Return firstDayOfWeek
            Case "LASTMONTH"
                Dim Day As DateTime
                Day = d.AddDays(d.Day * -1 + 1).AddMonths(-1)
                Return Day
            Case "THISMONTH"
                Dim Day As DateTime
                Day = d.AddDays(d.Day * -1 + 1)
                Return Day
            Case "LASTYEAR"
                Dim LastYearStart As DateTime
                LastYearStart = d.AddDays((d.DayOfYear - 1) * -1).AddYears(-1)
                Return LastYearStart
            Case "THISYEAR"
                Dim ThisYearStart As DateTime
                ThisYearStart = d.AddDays((d.DayOfYear - 1) * -1)
                Return ThisYearStart
            Case "THISWEEKLASTYEAR"
                Dim ThisWeekLastYear As DateTime
                Dim thisDayLastYear As DateTime = Today.AddDays(-365)
                Dim interval As Int32 = (thisDayLastYear.DayOfWeek - 1) * -1

                ThisWeekLastYear = thisDayLastYear.AddDays(interval)

                Return ThisWeekLastYear
            Case "LASTWEEKLASTYEAR"
                Dim ThisWeekLastYear As DateTime
                Dim thisDayLastYear As DateTime = Today.AddDays(-372)
                Dim interval As Int32 = (thisDayLastYear.DayOfWeek - 1) * -1

                ThisWeekLastYear = thisDayLastYear.AddDays(interval)

                Return ThisWeekLastYear
	Case "ALL"
	  Dim allDate as DateTime = #01/01/1900#
	return allDate
        End Select

    End Function

    Function GetPeriodTo(returnValue As String)

        'THISWEEKTO
        'LASTWEEKTO
        'LASTMONTHTO
        'THISMONTHTO
        'LASTYEARTO
        'THISYEARTO
        'THISWEEKLASTYEARTO
        Dim d As DateTime = Today()

        'Internal, manipulated returnValue
        Dim rVI As String = returnValue.ToUpper

        Select Case rVI
            Case "THISWEEK"
                Dim lastDayOfWeek As DateTime
                lastDayOfWeek = d.AddDays(DayOfWeek.Monday - d.DayOfWeek).AddDays(6)
                Return lastDayOfWeek
            Case "LASTWEEK"
                Dim lastDayOfWeek As DateTime
                lastDayOfWeek = d.AddDays(DayOfWeek.Monday - d.DayOfWeek).AddDays(-1)
                Return lastDayOfWeek
            Case "LASTMONTH"
                Dim Day As DateTime
                Day = d.AddDays(d.Day * -1 + 1).AddMilliseconds(-1)
                Return Day
            Case "THISMONTH"
                Dim Day As DateTime
                Day = d.AddDays(d.Day * -1 + 1).AddMonths(1).AddMilliseconds(-1)
                Return Day
            Case "LASTYEAR"
                Dim LastYearEnd As DateTime
                LastYearEnd = d.AddDays((d.DayOfYear - 1) * -1).AddMilliseconds(-1)
                Return LastYearEnd
            Case "THISYEAR"
                Dim LastYearEnd As DateTime
                LastYearEnd = d.AddDays((d.DayOfYear - 1) * -1).AddMilliseconds(-1).AddYears(1)
                Return LastYearEnd
            Case "THISWEEKLASTYEAR"
                Dim ThisWeekLastYear As DateTime
                Dim thisDayLastYear As DateTime = Today.AddDays(-365)
                Dim interval As Int32 = (7 - thisDayLastYear.DayOfWeek)

                ThisWeekLastYear = thisDayLastYear.AddDays(interval)

                Return ThisWeekLastYear
            Case "LASTWEEKLASTYEAR"
                Dim ThisWeekLastYear As DateTime
                Dim thisDayLastYear As DateTime = Today.AddDays(-372)
                Dim interval As Int32 = (7 - thisDayLastYear.DayOfWeek)

                ThisWeekLastYear = thisDayLastYear.AddDays(interval)

                Return ThisWeekLastYear

	Case "ALL"
	  Dim allDate as DateTime = Now()
	return allDate

        End Select

    End Function</Code>
