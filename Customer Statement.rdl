  <DataSets>
    <DataSet Name="CustomerStatement">
      <Query>
        <DataSourceName>Embedded</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DateFrom">
            <Value>=Parameters!DateFrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateFilterParam">
            <Value>=Parameters!DateFilterParam.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateTo">
            <Value>=Parameters!DateTo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CustomerFilter">
            <Value>=Parameters!CustomerFilter.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
	0 as TotalGrouping,
	CustRef,
	CustName,
	ClassID,
	DocumentNumber,
	Type,
	Date,
	Case when DueDate &lt; '2010/01/01' then null else duedate end as DueDate,
	OriginalTrxAmount,
	CurrentTrxAmount,
	[Current],
	Days31_60,
	Days61_90,
	Days91_180,
	Days181_270,
	Days271_364,
	Days365_Plus,
	[Current] + Days31_60 + Days61_90 + Days91_180 + Days181_270 + Days271_364 + Days365_Plus as OutstandingBalance
from 
	OutstandingInvoicesExtended
Where ((@DateFilterParam = 0 and Date between @DateFrom and @DateTo or DueDate &lt; '2010/01/01') or (@DateFilterParam = 1 and DueDate between @DateFrom and @DateTo or DueDate &lt; '2010/01/01'))
and (@CustomerFilter ='' or CustRef like '%'+@CustomerFilter+'%' or CustName like '%'+@CustomerFilter+'%')</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
